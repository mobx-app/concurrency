{
  "compilerOptions": {
    "target": "es5",
    "outDir": "lib",
    "sourceMap": false,
    "declaration": true,
    "module": "commonjs",
    "removeComments": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "lib": [
      "es5",
      "es2015.promise",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "test",
    "lib"
  ]
}
